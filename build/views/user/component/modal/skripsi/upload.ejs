<div
  class="modal fade"
  id="upload"
  tabindex="-1"
  aria-labelledby="uploadLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-primary" id="uploadLabel"><i class="bi bi-chevron-right"></i> Upload Skripsi</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <form method="post" action="/skripsi" enctype="multipart/form-data">
            <div class="form-floating mb-3">
              <input type="text" class="form-control" id="title" name="title" placeholder=" " required>
              <label for="title" class="form-label">Judul</label>
            </div>
            <div class="form-floating mb-3">
              <input type="text" class="form-control" id="author" name="author" placeholder=" " required>
              <label for="author" class="form-label">Penulis</label>
            </div>
            <div class="form-floating mb-3">
              <input type="number" class="form-control" id="year" name="year" placeholder=" " required>
              <label for="year" class="form-label">Tahun</label>
            </div>
            <div class="form-floating mb-3">
              <textarea class="form-control" name="abstract" id="abstrak" placeholder=" " required style="height: 125px; resize: none;"></textarea>
              <label for="abstrak">Abstrak</label>
            </div>
            <div class="form-floating mb-3">
              <input type="text" class="form-control" id="keyword" name="keyword" placeholder=" " required>
              <label for="keyword" class="form-label">Kata Kunci</label>
              <div id="fileHelp" class="form-text">
                *contoh, kata, kunci
              </div>
            </div>
            <div class="mb-3">
                <!-- <label for="year" class="form-label">Pembimbing I</label> -->
                <select class="form-select" name="pembimbingI" id="pembimbing" required>
                  <option selected disabled hidden>Pembimbing I</option>
                  <option value="Vivi Peggie Rantung, ST, MISD">Vivi Peggie Rantung, ST, MISD</option>
                  <option value="Cindy P. C. Munaiseche, S.Kom, M.Eng">Cindy P. C. Munaiseche, S.Kom, M.Eng</option>
                  <option value="Dr. Ing. Parabelem T. D. Rompas, MT">Dr. Ing. Parabelem T. D. Rompas, MT</option>
                  <option value="Gladly C. Rorimpandey, ST. MISD">Gladly C. Rorimpandey, ST. MISD</option>
                  <option value="Quido C. Kainde, ST, MT">Quido C. Kainde, ST, MT</option>
                  <option value="Olivia Kembuan, S.Kom, M.Eng">Olivia Kembuan, S.Kom, M.Eng</option>
                  <option value="Sondy Kumajas, ST, MT">Sondy Kumajas, ST, MT</option>
                  <option value="Kristofel Santa, S.ST, M.MT">Kristofel Santa, S.ST, M.MT</option>
                  <option value="Made Krisnanda, ST. MT">Made Krisnanda, ST. MT</option>
                  <option value="Ferdinan I. Sangkop, ST, MT">Ferdinan I. Sangkop, ST, MT</option>
                  <option value="Dr. Irene R.H Tangkawarow, ST, MISD">Dr. Irene R.H Tangkawarow, ST, MISD</option>
                  <option value="Dr. Audy A. Kenap, ST, M.Eng">Dr. Audy A. Kenap, ST, M.Eng</option>
                  <option value="Stralen Pratasik, S.Kom, MT">Stralen Pratasik, S.Kom, MT</option>
                  <option value="Efraim R. S. Moningkey, ST, MT">Efraim R. S. Moningkey, ST, MT</option>
                  <option value="Glenn D. P. Maramis, ST, MT">Glenn D. P. Maramis, ST, MT</option>
                </select>
            </div>
            <div class="mb-3">
                <!-- <label for="year" class="form-label">Pembimbing II</label> -->
                <select class="form-select" name="pembimbingII" id="pembimbing" required>
                  <option selected disabled hidden>Pembimbing II</option>
                  <option value="Vivi Peggie Rantung, ST, MISD">Vivi Peggie Rantung, ST, MISD</option>
                  <option value="Cindy P. C. Munaiseche, S.Kom, M.Eng">Cindy P. C. Munaiseche, S.Kom, M.Eng</option>
                  <option value="Dr. Ing. Parabelem T. D. Rompas, MT">Dr. Ing. Parabelem T. D. Rompas, MT</option>
                  <option value="Gladly C. Rorimpandey, ST. MISD">Gladly C. Rorimpandey, ST. MISD</option>
                  <option value="Quido C. Kainde, ST, MT">Quido C. Kainde, ST, MT</option>
                  <option value="Olivia Kembuan, S.Kom, M.Eng">Olivia Kembuan, S.Kom, M.Eng</option>
                  <option value="Sondy Kumajas, ST, MT">Sondy Kumajas, ST, MT</option>
                  <option value="Kristofel Santa, S.ST, M.MT">Kristofel Santa, S.ST, M.MT</option>
                  <option value="Made Krisnanda, ST. MT">Made Krisnanda, ST. MT</option>
                  <option value="Ferdinan I. Sangkop, ST, MT">Ferdinan I. Sangkop, ST, MT</option>
                  <option value="Dr. Irene R.H Tangkawarow, ST, MISD">Dr. Irene R.H Tangkawarow, ST, MISD</option>
                  <option value="Dr. Audy A. Kenap, ST, M.Eng">Dr. Audy A. Kenap, ST, M.Eng</option>
                  <option value="Stralen Pratasik, S.Kom, MT">Stralen Pratasik, S.Kom, MT</option>
                  <option value="Efraim R. S. Moningkey, ST, MT">Efraim R. S. Moningkey, ST, MT</option>
                  <option value="Glenn D. P. Maramis, ST, MT">Glenn D. P. Maramis, ST, MT</option>
                </select>
            </div>
            <div class="mb-3">
                <!-- <label for="file" class="form-label">Upload</label> -->
                <input type="file" class="form-control" id="file" name="file" accept=".pdf" required>
                <div id="fileHelp" class="form-text">
                    *upload file dengan format .pdf
                </div>
            </div>
        </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-primary" data-bs-dismiss="modal">Batal</button>
                <button type="submit" class="btn btn-primary"><i class="bi bi-upload"></i> Upload</button>
            </div>
        </form>
    </div>
    </div>
  </div>
</div>

