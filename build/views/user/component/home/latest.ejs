<div class="container-fluid" data-aos="fade-down" data-aos-duration="750">
  <div class="row justify-content-center py-5">
    <div class="col-8 text-start">
      <h4 class="text-dark mb-5 display-6">Terakhir Ditambahkan :</h4>
      <% if (skripsi.length < 1) { %>
      <h1 class="text-center text-primary display-6">-</h1>
      <% } %>
      <!--  -->
      <% skripsi.forEach(skripsi => { %>
      <div class="mb-5" data-aos="fade-up" data-aos-duration="750">
        <a
          class="text-decoration-none"
          href="/skripsi/detail/<%= skripsi.id %>"
        >
          <h5 class="text-primary">
            <%= skripsi.year %> | <%= skripsi.title %>
          </h5>
        </a>
        <p class="text-dark"><%= skripsi.author %></p>
        <p><%= skripsi.pembimbingI %> | <%= skripsi.pembimbingII %></p>
        <p><%= skripsi.abstract %></p>
        <hr />
      </div>
      <% }) %>
    </div>
  </div>
</div>
